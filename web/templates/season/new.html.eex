<div class="row">
  <div class="col-xs-12 col-sm-8 col-sm-offset-2">
    <div class="jumbotron content-box">
      <h2><%= gettext("create_new_season") %></h2>

      <p><%= gettext("create_new_season_notice") %></p>

      <%= render "form.html", changeset: @changeset, action: season_path(@conn, :create), years: @years %>

      <%= link gettext("all") <> " " <> gettext("seasons"), to: season_path(@conn, :index) %>
    </div>
  </div>
</div>
